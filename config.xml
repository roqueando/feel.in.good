<?xml version='1.0' encoding='utf-8'?>
<widget id="feel.in.good" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>feel.in</name>
    <description>
        A feeling app for anyone wants to share your expressions
    </description>
    <author email="vitor.rpereira@hotmail.com" href="http://roqueando.github.io">
        VÃ­tor Roque Pereira
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon density="mdpi" height="57" platform="ios" src="res/icon/android/feel_launcher_xhdpi.png" width="57" />
    <engine name="android" spec="^7.0.0" />
</widget>
